{
    "name": "hire-linker-frontpage",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "build": "next build",
        "dev": "PORT=8003 next dev",
        "lint:check": "npm run lint:check:eslint && npm run lint:check:prettier",
        "lint:check:eslint": "npx eslint --ext .ts,.tsx,.js,.jsx .",
        "lint:check:prettier": "npx prettier --check './**/*.{ts,tsx,js,jsx,md,json,css,scss,less}'",
        "lint:fix": "npm run lint:fix:eslint && npm run lint:fix:prettier",
        "lint:fix:eslint": "npx eslint --fix --ext .ts,.tsx,.js,.jsx .",
        "lint:fix:prettier": "npx prettier --write './**/*.{ts,tsx,js,jsx,md,json,css,scss,less}'",
        "prepare": "husky install",
        "start": "next start"
    },
    "dependencies": {
        "next": "14.1.3",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "sass": "1.71.1"
    },
    "devDependencies": {
        "@commitlint/cli": "19.0.3",
        "@commitlint/config-conventional": "19.0.3",
        "@types/node": "20.11.25",
        "@types/react": "18.2.64",
        "@types/react-dom": "18.2.21",
        "autoprefixer": "10.4.18",
        "eslint": "8.57.0",
        "eslint-config-next": "14.1.3",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-prettier": "5.1.3",
        "husky": "9.0.11",
        "lint-staged": "15.2.2",
        "postcss": "8.4.35",
        "prettier": "3.2.5",
        "tailwindcss": "3.4.1",
        "typescript": "5.4.2"
    }
}
